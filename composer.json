{
  "name": "zoparga/szamlazz-hu-szamla-agent",
  "license": "MIT",
  "description": "Szamlazz.hu PHP integration, package version",
  "keywords": [
    "laravel",
    "invoice",
    "receipt",
    "szamlazz.hu",
    "szamlazzhu",
    "currency",
    "php"
  ],
  "authors": [
    {
      "name": "Zolt√°n Papp",
      "email": "zoltan@pappz.dev"
    }
  ],
  "require": {
    "php": "^7.1.3|^8.0 |^8.1",
    "ext-simplexml": "*",
    "ext-xmlwriter": "*",
    "guzzlehttp/guzzle": "6.* | 7.*",
    "laravel/framework": " 6.* | 7.* | 8.* | 9.*"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "mockery/mockery": "^1.4"
  },
  "autoload": {
    "files": [
    ],
    "psr-4": {
      "zoparga\\SzamlazzHuSzamlaAgent\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "zoparga\\SzamlazzHuSzamlaAgent\\Tests\\": "test/"
    }
  },
  "suggest": {
    "illuminate/validation": "5.7.*"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "aliases": {
      },
      "providers": [
        "zoparga\\SzamlazzHuSzamlaAgent\\Providers\\SzamlazzHuSzamlaAgentServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "phpunit"
  }
}
